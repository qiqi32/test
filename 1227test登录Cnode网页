import unittest
from selinium import webdriver

#测试账户:
#用户名：qiqi3
#密码：QIqi32.

class Cnode(unittest.TestCase):

    def setUp(self):
        self.Url = 'http://39.107.96.138:3000/'
        self.driver = webdriver.Chrome()
        self.driver.get(self.Url)

    def test_register(self):
        driver = self.driver   
        driver.find_element_by_css_selector('a href="/signin"]').click()
        driver.find_element_by_id('name').send_keys('qiqi3')
        driver.find_element_by_id('pass').send_keys('QIqi32.')
        # driver.find_element_by_class_name('span-primary')
        # driver.find_element_by_id('re_pass').send_keys('xxxxxx')
        # driver.find_element_by_id('email').send_keys('xxxxx')
        driver.find_element_by_css_selector('input[type="submit"]').click()

    def test_login(self):
        pass


    def tearDown(self):
        self.driver.save_screenshot('D:\01.png')
        self.driver.quit()
if __name__ == '__main__':
    unittest.main()
